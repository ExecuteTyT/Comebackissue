{
  "name": "verni-strahovku",
  "version": "1.0.0",
  "description": "Лендинг для возврата навязанных банковских услуг - вернистраховку.рф",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "build:css": "tailwindcss -i ./src/css/tailwind-input.css -o ./src/css/tailwind-output.css --minify",
    "watch:css": "tailwindcss -i ./src/css/tailwind-input.css -o ./src/css/tailwind-output.css --watch",
    "test": "echo \"Тесты будут добавлены позже\" && exit 0",
    "copy-docx": "node scripts/copy-docx-to-assets.js"
  },
  "keywords": [
    "возврат страховки",
    "навязанные услуги",
    "юридические услуги",
    "финансовое право"
  ],
  "author": "ООО \"ПЕРВЫЙ\"",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf-csrf": "^3.0.0",
    "dompurify": "^3.0.8",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsdom": "^23.2.0",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.10",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "tailwindcss": "^3.4.18"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
